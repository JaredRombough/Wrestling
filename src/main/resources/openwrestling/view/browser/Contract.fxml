<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1"
          fx:controller="openwrestling.view.browser.controller.ContractController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="100.0" prefWidth="100.0"/>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" percentWidth="100.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" percentHeight="100.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" percentHeight="100.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" percentHeight="100.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        <RowConstraints minHeight="10.0" percentHeight="100.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <Button fx:id="contractButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                mnemonicParsing="false" text="Sign Contract" GridPane.columnSpan="2147483647" GridPane.rowIndex="3">
            <GridPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </GridPane.margin>
        </Button>
        <Label fx:id="contractText" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
               prefWidth="136.0" styleClass="workerStat" text="Label" GridPane.columnIndex="1"/>
        <Label fx:id="contractLabel" alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308"
               maxWidth="1.7976931348623157E308" styleClass="workerStatLabel" text="Contract"/>
        <Label fx:id="contractDurationLabel" alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308"
               maxWidth="1.7976931348623157E308" styleClass="workerStatLabel" text="Remaining" GridPane.rowIndex="2"/>
        <Label fx:id="contractDurationText" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
               prefWidth="136.0" styleClass="workerStat" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
        <Label fx:id="contractTypeLabel" alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308"
               maxWidth="1.7976931348623157E308" styleClass="workerStatLabel" text="Terms" GridPane.rowIndex="1"/>
        <Label fx:id="contractTypeText" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
               prefWidth="136.0" styleClass="workerStat" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
    </children>
</GridPane>
