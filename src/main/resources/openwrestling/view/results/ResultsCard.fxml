<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchorPane" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="openwrestling.view.results.controller.ResultsCardController">
    <children>
        <VBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <StackPane fx:id="border" styleClass="imageBorder">
                    <children>
                        <ImageView fx:id="imageView" fitHeight="105.0" fitWidth="120.0" pickOnBounds="true"
                                   preserveRatio="true"/>
                    </children>
                </StackPane>
                <Label fx:id="nameLabel" alignment="TOP_LEFT" text="Label"/>
            </children>
        </VBox>
    </children>
</AnchorPane>
