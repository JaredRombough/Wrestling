<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchorPane" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="openwrestling.view.results.controller.ResultsCardController">
    <children>
        <StackPane fx:id="border" styleClass="imageBorder" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <ImageView fx:id="imageView" pickOnBounds="true" preserveRatio="true"/>
            </children>
        </StackPane>
        <Label fx:id="nameLabel" alignment="TOP_LEFT" style="-fx-background-color: black;" text="Label" textFill="WHITE"
               AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"/>
    </children>
</AnchorPane>
